<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hooptap.brandsampleaws" >
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <application
        android:allowBackup="true"
        android:name=".HTApplication"
        android:icon="@drawable/hooptap"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".Principal" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Generic.HooptapActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Register"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Login"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Profile"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".UpdateUser"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".BadgeList"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".BadgeDetail"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Actions"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Feed"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Initial"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".ItemList"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".ItemDetail"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".RankingDetail"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".RankingList"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".LevelDetail"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".LevelList"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Rewards"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".QuestsList"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".QuestDetail"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Details_Badge_Level_Quest"
            android:screenOrientation="portrait"></activity>
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>


</manifest>
